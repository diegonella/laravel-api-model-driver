{
    "name": "diegonella/laravel-api-model-driver",
    "description": "API driver for Laravel Eloquent models",
    "keywords": [
        "laravel",
        "database driver",
        "api",
        "eloquent"
    ],
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "illuminate/database": "~5.7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Gonella",
            "email": "diegonella@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "diegonella\\LaravelApiModelDriver\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "diegonella\\LaravelApiModelDriver\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
